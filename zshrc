#! /bin/zsh

# Some Aliases
alias ys='yarn start'
alias c.='code .'
alias hl='heroku local'
alias kan='killall node'

export ZSH=$HOME/.oh-my-zsh
export EDITOR='nvim'
export CONFIG_FILES_PATH=$HOME/Documents/configFiles
ZSH_THEME="robbyrussell"

plugins=(
  git
)

# source $ZSH/oh-my-zsh.sh

